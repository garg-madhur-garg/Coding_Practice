#include <bits/stdc++.h>

using namespace std;

int main()
{
    string s;
    cin >> s;
    
    int n = s.length();
    
    unordered_map<char, int> mp;
    
    for(int i = 0; i < n; i++){
        mp[s[i]]++;
    }
    
    for(auto x:mp){
        if(x.second > 1){
            cout << x.first << " comes for " << x.second << " times" << endl;
        }
    }
    return 0;
}
