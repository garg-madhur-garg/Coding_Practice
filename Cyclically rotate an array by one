void rotate(int arr[], int n)
{
    int temp = arr[n - 1];
    for(int i = n - 1; i > 0; i--){
        arr[i] = arr[i - 1];
    }
    arr[0] = temp;
}


----------------------------------OR--------------------------------------

void rotate(int arr[], int n)
{
    int i = 0;
    int j = n - 1;
    
    while(i != j){
        swap(arr[i], arr[j]);
        i++;
    }
    
}
