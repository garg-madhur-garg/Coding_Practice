//Function to find the lowest common ancestor in a BST. 
Node* LCA(Node *root, int n1, int n2)
{
   //Your code here
   if(root == NULL){
       return NULL;
   }
   
   int curr = root->data;
   if(curr < n1 && curr < n2){
       return LCA(root->right, n1, n2);
   }
   else if(curr > n1 && curr > n2){
       return LCA(root->left, n1, n2);
   }
   else{
       return root;
   }
}
